{
  "extends": ["../../.eslintrc.json"],
  "ignorePatterns": ["!**/*"],
  "overrides": [
    {
      "files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
      "rules": {
        "@typescript-eslint/no-empty-function": "off"
      }
    },
    {
      "files": ["*.ts__template__"],
      "extends": ["plugin:@nx/typescript", "plugin:prettier/recommended"]
    },
    {
      "files": ["*.js", "*.jsx"],
      "rules": {}
    }
  ]
}
